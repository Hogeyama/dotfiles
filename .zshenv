alias pandoc-md='pandoc -s -f markdown+lists_without_preceding_blankline+ignore_line_breaks'
alias pandoc-md-xe='pandoc -s -f markdown+lists_without_preceding_blankline+ignore_line_breaks --template=xelatex.latex'
alias pandoc-md-beamer='pandoc -s -f markdown+lists_without_preceding_blankline+ignore_line_breaks -t beamer'
alias open='xdg-open'
alias node='nodejs'
alias stack-ghcid='stack exec -- ghcid "--command=stack ghci"'
